# Server Configuration
PORT=3000

# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Google Vertex AI Configuration
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_MCP_SERVER=https://api.githubcopilot.com/mcp/

# GitHub App Configuration
GITHUB_APP_ID=your_github_app_id
GITHUB_APP_PRIVATE_KEY=your_github_app_private_key_pem_format
GITHUB_APP_INSTALLATION_ID=your_github_app_installation_id

# GitHub API Configuration (Optional)
# For GitHub Enterprise Server, set the base URL (e.g., https://github.company.com/api/v3)
# GITHUB_API_BASE_URL=https://api.github.com
# GITHUB_API_VERSION=2022-11-28

# Atlassian Configuration
ATLASSIAN_MCP_SERVER=your-mcp-server-url
ATLASSIAN_DOMAIN=your-domain.atlassian.net

# Atlassian Basic Auth Configuration
ATLASSIAN_EMAIL=your-email
ATLASSIAN_API_TOKEN=your-token

# Atlassian OAuth Configuration
ATLASSIAN_CLIENT_ID=your-service-account-client-id
ATLASSIAN_CLIENT_SECRET=your-service-account-client-secret
ATLASSIAN_CLOUD_ID=your-atlassian-cloud-id

CONFLUENCE_SPACE_KEY=your_confluence_space_key
